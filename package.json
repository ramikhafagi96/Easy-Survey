{
  "name": "Easy-Survey",
  "version": "1.0.0",
  "description": "A web application used by startups to collect feedbacks about their products easily.",
  "main": "app.js",
  "engines": {
    "node": "12.18.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
