    "@sendgrid/mail": "^7.2.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.3",
    "loadash": "^1.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.18",
